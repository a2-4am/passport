; MLI error $28
;
.writenodev
         !text "S%S,D%D does not exist.",$8D,$8D,$00
;
; MLI error $2B
;
.writeprot
         !text "The disk in S%S,D%D is write-protected.",$8D,$8D,$00
;
; other MLI error
;
.othermli
         !text "MLI error $%0.",$8D
         !text "Please contact the developer.",$8D,$8D,$00

