;-------------------------------
; #DATASOFT
; nibble count in encrypted bootloader
; e.g. Video Title Shop, Tomahawk //e
;-------------------------------
!zone {
         lda   gIsDatasoft
         bne   .exit

         ldy   #$06
         jsr   SearchTrack
         !byte $5D,$D4,$80,$49,00,$19
         bcs   .exit
         sta   gDisplayBytes
         pha
         lda   #s_datasoft
         jsr   PrintByID
         pla
         ldy   #$01
         jsr   modify
         !byte $C0
.exit
}
